<% provide(:title, 'Form view View (Student)') %>

<div class="container">
 <%= form_with(model: @peer_eval, url: team_project_peer_eval_path(id: @user.id), local: true) do |f| %>
  <%= f.label :score %>
  <%= f.text_field :score %>

  <%= f.label :comment %>
  <%= f.text_field :comment %>

  <%= f.submit "Submit peer evaluation" %>

 <% end %>
</div> 